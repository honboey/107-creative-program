{
  "Main": {
    "event_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading3",
        "label": "Title",
        "placeholder": "e.g Out of Iso"
      }
    },
    "link": {
      "type": "Link",
      "config": {
        "label": "Link",
        "placeholder": "Link to listing e.g https://107.org.au/event/...",
        "select": null
      }
    },
    "time_and_date": {
      "type": "StructuredText",
      "config": {
        "single": "paragraph",
        "label": "Time and date",
        "placeholder": "e.g 6pm, 12 Jan 2022"
      }
    },
    "month_number": {
      "type": "Number",
      "config": {
        "label": "Month number",
        "min": 0,
        "max": 12
      }
    },
    "location": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph",
        "label": "Location",
        "placeholder": "e.g online, Performance space etc"
      }
    },
    "location_link": {
      "type": "Link",
      "config": {
        "label": "Location link",
        "placeholder": "e.g Link to Google map",
        "select": null
      }
    },
    "cost": {
      "type": "StructuredText",
      "config": {
        "single": "paragraph",
        "label": "Cost",
        "placeholder": "e.g Free, $20 etc"
      }
    },
    "event_image": {
      "type": "Image",
      "config": {
        "constraint": {},
        "thumbnails": [],
        "label": "event image"
      }
    },
    "descriptor": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph,strong,em,hyperlink",
        "label": "Descriptor"
      }
    },
    "ticket_link": {
      "type": "Link",
      "config": {
        "label": "Ticket link",
        "select": null
      }
    }
  }
}